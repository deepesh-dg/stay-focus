{
    "name": "Stay Focus",
    "version": "0.1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "css": [],
            "js": [
                "./stayfocus.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
} 